<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="module_category_special_date" model="ir.module.category">
            <field name="name">特殊日期</field>
        </record>

        <record id="group_special_date_user" model="res.groups">
            <field name="name">用户</field>
            <field name="category_id" ref="special_date.module_category_special_date"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

        <record id="group_special_date_manager" model="res.groups">
            <field name="name">管理员</field>
            <field name="category_id" ref="special_date.module_category_special_date"/>
            <field name="implied_ids"
                    eval="[(4, ref('base.group_user')), (4, ref('base.user_admin'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

    </data>
</odoo>